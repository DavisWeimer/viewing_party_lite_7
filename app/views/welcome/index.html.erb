<% if current_user %>
  <%= button_to "Log Out", "/logout", method: :get %>
<% else %>
  <div id="new_user">
    <%= button_to "Create New User", "/register", method: :get %>
  </div>
  <div id="login_user">
    <%= button_to "Log In", "/login", method: :get %>
  </div>
<% end %>

<ul id="existing_users_emails">
  <h3>User Emails</h3>
  <% @users.each do |user| %>
    <li><%= user.email %></li>
  <% end %>
</ul> 